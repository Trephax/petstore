{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "object-curly-spacing": ["error", "always"],
        "indent": ["error", 2],
        "max-len": "off",
        "require-jsdoc": "off",
        "react/react-in-jsx-scope": "off",
        "react/jsx-space-before-closing": ["error", "always"],
        "no-unused-vars": ["warn"]
    }
}
